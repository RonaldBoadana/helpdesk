{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "--jsx": "react",
    "strict": false,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
    }
  },
}